{
  "name": "lodash-import-what-you-need",
  "publisher": "pilotkid",
  "displayName": "lodash-import-what-you-need",
  "description": "Changes ES6 lodash imports to only import what is used in your code",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.57.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:lodash-import-what-you-need.lodash-treeshake"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "lodash-import-what-you-need.lodash-treeshake",
        "title": "Lodash Shake"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.2",
    "@types/node": "14.x",
    "@types/vscode": "^1.57.0",
    "eslint": "^7.27.0",
    "glob": "^7.1.7",
    "mocha": "^8.4.0",
    "typescript": "^4.3.2",
    "vscode-test": "^1.5.2"
  }
}
